{
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./.tsbuildinfo",
    "target": "es2016",
    "module": "commonjs",
    "paths": {
      "hkt-toolbelt": ["./src"],
      "hkt-toolbelt/$": ["./src/components/$"],
      "hkt-toolbelt/boolean": ["./src/components/boolean"],
      "hkt-toolbelt/cast": ["./src/components/Cast"],
      "hkt-toolbelt/conditional": ["./src/components/Conditional"],
      "hkt-toolbelt/function": ["./src/components/Function"],
      "hkt-toolbelt/kind": ["./src/components/Kind"],
      "hkt-toolbelt/list": ["./src/components/List"],
      "hkt-toolbelt/string": ["./src/components/String"],
      "hkt-toolbelt/test": ["./src/components/Test"]
    },
    "rootDirs": ["src", "test"],
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outFile": "dist/index.ts",
    "outDir": "./dist/",
    "stripInternal": true,
    "noEmitOnError": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,
    "strict": true
  }
}
